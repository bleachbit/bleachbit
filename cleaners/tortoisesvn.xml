<cleaner id="tortoisesvn" os="windows">
  <label>TortoiseSVN</label>
  <option id="history">
    <label>History</label>
    <description>Delete the usage history</description>
    <action command="winreg" path="HKCU\Software\TortoiseSVN\History"/>
  </option>
</cleaner>
