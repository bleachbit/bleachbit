<!-- verified with Octave version 3.6.2 on Ubuntu 12.10 -->
<!-- verified with Octave version 3.6.2 on Windows XP -->
<cleaner id="octave">
  <label>Octave</label>
  <option id="history">
    <label>History</label>
    <description>Delete the command history</description>
    <action command="delete" search="file" path="~/.octave_hist"/>
  </option>
</cleaner>
