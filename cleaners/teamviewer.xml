<cleaner id="tv" os="windows">
  <label>TeamViewer</label>
  <description>A free remote desktop program</description>
  <option id="logs">
    <label>Logs</label>
    <description>Delete the logs</description>
    <action type="glob">$APPDATA\TeamViewer\*log</action>
  </option>
  <option id="connections">
    <label>Connections</label>
    <description>Delete the list of connections</description>
    <action type="file">$APPDATA\TeamViewer\Connections.txt</action>
  </option>
</cleaner>
