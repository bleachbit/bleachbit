<?xml version="1.0" encoding="UTF-8"?>

<cleaner id="windows_dism" os="windows">
  <label>Windows DISM</label>
  <description>DISM</description>
  <option id="dism">
    <label>Run dism.exe</label>
    <description>Delete the dism.exe files</description>
    <action command="process" wait="true" cmd="dism.exe /online /cleanup-image /spsuperseded"/>
  </option>
</cleaner>
